<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zyc.demo.redis.mapper.TbLockMapper">

    <insert id="insertData">
        insert into tb_lock
        (id)
        values
        (#{id})
    </insert>

    <delete id="del">
        delete from
        tb_lock
        where id = #{id}
    </delete>
</mapper>